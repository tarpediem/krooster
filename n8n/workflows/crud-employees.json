        "query": "={{ 'UPDATE employees SET ' + Object.entries($json.body).filter(([k,v]) => k !== 'id' && v !== undefined).map(([k,v]) => { if (k === 'positions') return `positions = ARRAY[${v.map(p => \"'\" + p + \"'\").join(',')}]::TEXT[]`; if (k === 'is_mobile') return `is_mobile = ${v}`; if (k === 'restaurant_id') return `restaurant_id = ${v}`; if (k === 'fixed_day_off') return v === null ? 'fixed_day_off = NULL' : `fixed_day_off = ${v}`; if (k === 'max_hours_per_week') return v === null ? 'max_hours_per_week = NULL' : `max_hours_per_week = ${v}`; return `${k} = '${v}'`; }).join(', ') + ` WHERE id = ${$json.body.id} RETURNING *;` }}",
